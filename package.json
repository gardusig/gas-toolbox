{
  "name": "gas-toolbox",
  "version": "1.0.0",
  "description": "A comprehensive Google Apps Script library for Drive, Docs, and Sheets",
  "scripts": {
    "build": "tsc && node scripts/remove-exports.js && cp appsscript.json dist/appsscript.json",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' 'tests/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts' 'tests/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts' 'tests/**/*.ts'",
    "pre-test": "npm run lint && npm run format:check",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text --coverageReporters=lcov --coverageReporters=html --coverageReporters=json-summary",
    "deploy": "npm run build && clasp push",
    "deploy:force": "npm run build && clasp push -f",
    "version": "clasp version",
    "versions": "clasp versions",
    "publish": "node scripts/publish-version.js"
  },
  "keywords": [
    "google-apps-script",
    "google-drive",
    "google-sheets",
    "data-mapping",
    "spreadsheet"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/google-apps-script": "^1.0.78"
  },
  "devDependencies": {
    "@jest/test-sequencer": "^29.7.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "esbuild": "^0.27.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "js-yaml": "^4.1.0",
    "node-notifier": "^10.0.1",
    "braces": "^3.0.3",
    "micromatch": "^4.0.8",
    "anymatch": "^3.1.3",
    "sane": "^5.0.1",
    "form-data": "^4.0.0",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.11",
    "tough-cookie": "^4.1.3"
  }
}

